# Environment Variables (copy to .env and fill with your values)
PRIVATE_KEY=
SEPOLIA_URL=https://sepolia.infura.io/v3/
MAINNET_URL=https://mainnet.infura.io/v3/
ETHERSCAN_API_KEY=
REPORT_GAS=true

# Factory deploy config
# Account that will receive creation fees from the factory
FEE_RECIPIENT=
# Creation fee (in ETH) charged when creating a new fund
CREATION_FEE_ETHER=0.01

# Default fund parameters for create-fund script
FUND_NAME=Crypto Index
FUND_SYMBOL=CNDX
MGMT_FEE_BPS=200
PERF_FEE_BPS=1000

# Frontend (Next.js) public env
NEXT_PUBLIC_INFURA_ID=your_infura_id
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id
NEXT_PUBLIC_FACTORY_ADDRESS=0xYourFactoryOnNetwork
NEXT_PUBLIC_UNI_V3_ROUTER=0xYourUniswapV3Router
NEXT_PUBLIC_WETH=0xYourWETH

# Optional DEX config for deploy/create-fund scripts
NEXT_PUBLIC_UNI_V3_ROUTER=
NEXT_PUBLIC_WETH=

# Chainlink Price Feed Addresses (Sepolia Testnet)
ETH_USD_PRICE_FEED=0x694AA1769357215DE4FAC081bf1f309aDC325306
BTC_USD_PRICE_FEED=0x1b44F3514812d835EB1BDB0acB33d3fA3351Ee43
LINK_USD_PRICE_FEED=0xc59E3633BAAC79493d908e63626716e204A45EdF
